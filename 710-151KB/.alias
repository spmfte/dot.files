alias c="clear"
alias cdc="cd && clear"
alias i3config="nvim .config/i3/config"
alias conf="nvim ~/.zshrc"
alias conf!="source ~/.zshrc"
alias pac="sudo pacman -S"
alias q="exit"
alias vi="nvim"
alias ls="lsd"
alias pS="sudo pacman -S"
alias flip="xrandr --output eDP1 --rotate inverted && xinput set-prop 9 'Coordinate Transformation Matrix' -1 0 1 0 -1 1 0 0 1"
alias flip!="xrandr --output eDP1 --rotate normal && xinput set-prop 9 'Coordinate Transformation Matrix' 1 0 0 0 1 0 0 0 1"
alias flip90="xrandr --output eDP1 --rotate right"
alias flip270="xrandr --output eDP1 --rotate left && xinput set-prop 9 'Coordinate Transformation Matrix' 0 -1 1 1 0 0 0 0 1"
alias lock="xlock -nolock -mode -random"
alias draw="cd /home/aidan/go/bin/ && ./draw"
alias scratch="nvim-scratchpad"
alias aa="add-alias"
alias battime="/home/aidan/scripts/sysutil/./battery_health"
alias chvibe="wal -i /home/aidan/wallpaper/"
alias shino="wal -i /home/aidan/wallpaper/shino.png"
alias tbd="tb -d"
alias update="/home/aidan/scripts/sysutil/./updatesys.sh"
alias sound="pavucontrol"
alias korafetchTV="cat ~/ascii-art/kora/bigkoratv.txt"
alias vim="nvim"
alias syu="sudo pacman -Syu"
alias rn="ranger"
